<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.leedga.seagate.leedga"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-permission android:name="com.android.vending.BILLING"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.leedga.seagate.leedga.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.TestActivity"
            android:label="@string/title_activity_test"
            android:parentActivityName="com.leedga.seagate.leedga.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.leedga.seagate.leedga.MainActivity"/>
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.ResultActivity"
            android:label="@string/title_activity_result"
            android:parentActivityName="com.leedga.seagate.leedga.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.leedga.seagate.leedga.MainActivity"/>
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.TestSettingActivity"
            android:label="@string/title_activity_test_type"
            android:parentActivityName="com.leedga.seagate.leedga.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.leedga.seagate.leedga.MainActivity"/>
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.HistoryActivity"
            android:label="@string/title_activity_history"
            android:parentActivityName="com.leedga.seagate.leedga.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.leedga.seagate.leedga.MainActivity"/>
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.AnswersActivity"
            android:label="@string/title_activity_answers"
            android:parentActivityName="com.leedga.seagate.leedga.ResultActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.leedga.seagate.leedga.ResultActivity"/>
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.AnswerViewActivity"
            android:label="@string/title_activity_answer_view_acivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.ReportActivity"
            android:label="@string/title_activity_report"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.LessonsActivity"
            android:label="@string/title_activity_lessons"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.leedga.seagate.leedga.SettingActivity"
            android:label="@string/title_activity_setting"
            android:parentActivityName="com.leedga.seagate.leedga.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.leedga.seagate.leedga.MainActivity"/>
        </activity>

        <activity
            android:name="com.vungle.publisher.VideoFullScreenAdActivity"
            android:configChanges="keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>

        <activity
            android:name="com.vungle.publisher.MraidFullScreenAdActivity"
            android:configChanges="keyboardHidden|orientation|screenSize|screenLayout|smallestScreenSize"
            android:theme=
                "@android:style/Theme.Translucent.NoTitleBar.Fullscreen"/>

        <receiver android:name="com.leedga.seagate.leedga.NotificationService"/>
    </application>

</manifest>